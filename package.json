{
  "name": "web",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:site": "gatsby develop",
    "dev:lambda": "netlify-lambda serve functions/lib/",
    "netlify": "netlify dev",
    "build": "run-p build:*",
    "build:site": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "build:lambda": "netlify-lambda build functions/lib/",
    "serve": "gatsby serve",
    "release": "standard-version"
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.16",
    "@mdx-js/react": "1.6.16",
    "gatsby": "2.24.57",
    "gatsby-plugin-mdx": "1.2.39",
    "gatsby-plugin-netlify": "2.3.14",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sass": "2.3.12",
    "gatsby-plugin-sitemap": "2.4.12",
    "gatsby-plugin-typescript": "2.4.19",
    "gatsby-plugin-typography": "2.5.10",
    "gatsby-source-filesystem": "2.3.29",
    "gatsby-source-graphql": "2.7.3",
    "gatsby-transformer-json": "2.4.11",
    "lodash": "4.17.20",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "sanitize.css": "12.0.1",
    "sass": "1.26.10",
    "stylelint-config-sass-guidelines": "7.1.0",
    "typeface-bitter": "0.0.71",
    "typeface-cambo": "0.0.72",
    "typescript": "4.0.2",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@sondr3/eslint-config-react": "0.5.7",
    "@sondr3/eslint-config-typescript": "0.8.3",
    "@sondr3/prettier": "0.2.1",
    "@sondr3/stylelint": "0.5.7",
    "@sondr3/tsconfig": "0.4.1",
    "@types/aws-lambda": "8.10.62",
    "@types/css-modules": "1.0.0",
    "@types/node": "14.10.1",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/typography": "0.16.3",
    "eslint": "7.8.1",
    "eslint-plugin-mdx": "1.8.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "1.1.12",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "netlify-cli": "2.62.0",
    "netlify-lambda": "2.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "serve": "11.3.2",
    "standard-version": "9.0.0",
    "stylelint": "13.7.1"
  },
  "prettier": "@sondr3/prettier",
  "browserslist": [
    "> 1% in NO",
    "not ie >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write"
    ]
  }
}
