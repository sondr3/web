{
  "name": "web",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "release": "standard-version"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "gatsby": "^2.18.12",
    "gatsby-plugin-mdx": "^1.0.62",
    "gatsby-plugin-netlify": "^2.1.30",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-plugin-typography": "^2.3.20",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-transformer-json": "^2.2.22",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "sanitize.css": "^11.0.0",
    "sass": "^1.23.7",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "typeface-bitter": "^0.0.71",
    "typeface-cambo": "^0.0.72",
    "typescript": "^3.7.3",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@sondr3/eslint-config-react": "^0.4.0",
    "@sondr3/eslint-config-typescript": "^0.4.1",
    "@sondr3/prettier": "^0.1.1",
    "@sondr3/stylelint": "^0.5.0",
    "@sondr3/tsconfig": "^0.3.1",
    "@types/css-modules": "^1.0.0",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.11",
    "@types/typography": "^0.16.3",
    "eslint": "^6.7.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "serve": "^11.2.0",
    "standard-version": "^7.0.1",
    "stylelint": "^12.0.0"
  },
  "prettier": "@sondr3/prettier",
  "browserslist": [
    "> 1% in NO",
    "not ie >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write",
      "git add"
    ]
  }
}
