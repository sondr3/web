{
  "name": "web",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --incremental"
  },
  "dependencies": {},
  "devDependencies": {
    "@sondr3/eslint-config-typescript": "0.8.4",
    "@sondr3/prettier": "0.2.1",
    "@sondr3/stylelint": "0.5.7",
    "@sondr3/tsconfig": "0.4.1",
    "@types/node": "14.11.1",
    "eslint": "7.9.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "serve": "11.3.2",
    "stylelint": "13.7.1",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "4.0.3"
  },
  "prettier": "@sondr3/prettier",
  "browserslist": [
    "> 1% in NO",
    "not ie >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write"
    ]
  }
}
