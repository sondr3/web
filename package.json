{
  "name": "web",
  "version": "0.3.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:site": "gatsby develop",
    "dev:lambda": "netlify-lambda serve functions/lib/",
    "netlify": "netlify dev",
    "build": "run-p build:*",
    "build:site": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "build:lambda": "netlify-lambda build functions/lib/",
    "serve": "gatsby serve",
    "release": "standard-version"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.9",
    "@mdx-js/react": "^1.5.9",
    "gatsby": "^2.20.36",
    "gatsby-plugin-mdx": "^1.1.11",
    "gatsby-plugin-netlify": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.2.4",
    "gatsby-plugin-sitemap": "^2.3.6",
    "gatsby-plugin-typescript": "^2.3.5",
    "gatsby-plugin-typography": "^2.4.4",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-json": "^2.3.4",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "sanitize.css": "^11.0.0",
    "sass": "^1.26.5",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typeface-bitter": "^0.0.71",
    "typeface-cambo": "^0.0.72",
    "typescript": "^3.7.3",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@sondr3/eslint-config-react": "^0.4.3",
    "@sondr3/eslint-config-typescript": "^0.5.2",
    "@sondr3/prettier": "^0.2.0",
    "@sondr3/stylelint": "^0.5.4",
    "@sondr3/tsconfig": "^0.4.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/css-modules": "^1.0.0",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.11",
    "@types/typography": "^0.16.3",
    "eslint": "^6.7.2",
    "eslint-plugin-mdx": "^1.6.9",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "netlify-cli": "^2.48.0",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serve": "^11.2.0",
    "standard-version": "^8.0.1",
    "stylelint": "^13.3.3"
  },
  "prettier": "@sondr3/prettier",
  "browserslist": [
    "> 1% in NO",
    "not ie >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,css,md,mdx}": [
      "prettier --write"
    ]
  }
}
