{
  "extends": "astro/tsconfigs/strictest",
  "include": ["src/**/*"],
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "types": ["astro/client"],
    "baseUrl": ".",
    "skipLibCheck": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
